{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Environment": {
        "EnvironmentName": "Production",
        "EnableVerboseLogging": false,
        "ExternalServiceTimeoutSeconds": 5,
        "AllowDiagnosticEndpoints": false
    }
}