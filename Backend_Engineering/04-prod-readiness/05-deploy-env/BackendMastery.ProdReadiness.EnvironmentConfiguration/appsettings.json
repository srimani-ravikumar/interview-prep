{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Environment": {
        "EnvironmentName": "Base",
        "EnableVerboseLogging": false,
        "ExternalServiceTimeoutSeconds": 10,
        "AllowDiagnosticEndpoints": false
    }
}
